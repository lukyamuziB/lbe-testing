{
    "name": "andela/lenken",
    "description": "The Andela Mentorship Project.",
    "keywords": ["andela", "lenken", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0",
        "laravel/lumen-framework": "5.5.*",
        "vlucas/phpdotenv": "~2.2",
        "predis/predis": "^1.1",
        "illuminate/redis": "^5.4",
        "wn/lumen-generators": "^1.3",
        "lcobucci/jwt": "^3.2",
        "peridot-php/peridot": "^1.19",
        "peridot-php/leo": "~1.0",
        "barryvdh/laravel-cors": "^0.9.2",
        "illuminate/mail": "5.5.*",
        "guzzlehttp/guzzle": "^6.3",
        "doctrine/dbal": "^2.5",
        "google/apiclient": "^2.2",
        "google/auth": "^1.0",
        "google/cloud-storage": "^1.2",
        "nordsoftware/lumen-newrelic": "^1.1",
        "nordsoftware/lumen-chained-exception-handler": "^1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~0.9",
        "laravel/homestead": "^5.2",
        "squizlabs/php_codesniffer": "^3.0",
        "phpmd/phpmd": "^2.6",
        "phpunit/phpunit": "6.0.*",
        "codeclimate/php-test-reporter": "^0.4.4",
        "filp/whoops": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/GoogleCalendarHelper.php",
            "app/Helpers/RequestFormatterHelper.php"
        ],
        "classmap": [ "./vendor/google/apiclient/src/Google" ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ],
        "files": [
            "app/Helpers/GoogleCalendarHelper.php",
            "app/Helpers/RequestFormatterHelper.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Test\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan generate:google-credentials"
        ],
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "test": "phpunit --debug --coverage-clover build/logs/clover.xml"
    },
    "config": {
        "process-timeout": 600
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
